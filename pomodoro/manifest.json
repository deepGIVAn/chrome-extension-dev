{
  "manifest_version": 3,
  "name": "Pomodoro-timer",
  "description": "Save your timer and get things done on time",
  "version": "1.0",
  "icons": {
    "16": "images/instagram-16.png",
    "32": "images/instagram-32.png",
    "48": "images/instagram-48.png",
    "128": "images/instagram-128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_icon": {
      "16": "images/instagram-16.png",
      "32": "images/instagram-32.png",
      "48": "images/instagram-48.png",
      "128": "images/instagram-128.png"
    }
  },
  "permissions": ["scripting", "activeTab"]
}
